{
    "compilerOptions": {
        "module": "Preserve",
        "strict": true,
        "esModuleInterop": true,
        "moduleDetection": "force",
        "allowJs": true,
        "isolatedModules": true,
        "resolveJsonModule": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules","dist"]
}